    "@types/node": "16.11.26",
    "aws-cdk": "2.17.0",
    "husky": "^7.0.4",
    "jest": "27.5.1",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "aws-cdk-lib": "2.17.0",
    "constructs": "10.0.95",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
